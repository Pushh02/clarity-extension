{
  "name": "clarity-clean",
  "displayName": "clarity clean",
  "description": "Clarity clean helps you write the clean clarity code with visual buttons and maps or the greater clarity.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "clarity",
        "aliases": [
          "clarity",
          "clarity"
        ],
        "extensions": [
          ".clar"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "clarity",
        "scopeName": "\\source.clarity",
        "path": "./syntaxes/clarity.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "clarity",
        "path": "./snippets/clarity.json"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
